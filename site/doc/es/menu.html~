<script>
var marked = null
function mark(elt){
    if(marked!==null){
        marked.style.color="#aaa"
        marked.style.fontWeight="normal"
    }
    if(elt===undefined){
        elt = document.getElementsByTagName('A')[0]
    }
    elt.style.color="#000"
    elt.style.fontWeight = "bold"
    marked=elt
}
</script>
</head>
<body onload="mark()">
